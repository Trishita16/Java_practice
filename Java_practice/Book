class Books{
    String title,author;
    int isbn;
    double price;

    Book(String title,String author,int isbn,int price)
    {
        this.title=title;
        this.author=author;
        this.isbn=isbn;
        this.price=price;
    }
    Book(int price,int isbn,String author,String title)
    {
        this.price=price;
        this.isbn=isbn;
        this.author=author;
        this.title=title;
    }
    void display()
    {
        System.out.println("Title"+title);
        System.out.println("Author"+author);
        System.out.println("ISBN"+isbn);
        System.out.println("Price"+price);
    }
    public static void main(String[] args) {
        Book b1=new Book("Gitanjali","Rabindranath",101,300);
        Book b2 = new Book("sesher Kobita","Rabindranath Tagor",102,200);
        b1.display();
        b2.display();
    }
}